{
  "kind": "collectionType",
  "collectionName": "containers",
  "info": {
    "singularName": "container",
    "pluralName": "containers",
    "displayName": "Container"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "enumeration",
      "enum": ["S", "M", "L"],
      "required": true,
      "default": "M"
    },
    "has_camera": {
      "type": "boolean",
      "default": false
    },
    "internal_camera_id": {
      "type": "string"
    },
    "availability_status": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "available",
        "reserved",
        "rented"
      ]
    }
  }
}
